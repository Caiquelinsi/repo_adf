{
	"name": "pipe_copydata",
	"properties": {
		"activities": [
			{
				"name": "exel_to_csv",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "espera",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_excel_fixo",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_dinamico_csv",
						"type": "DatasetReference",
						"parameters": {
							"containerName": "csvfiless",
							"fileName": "csvCor.csv",
							"separetorName": ";"
						}
					}
				]
			},
			{
				"name": "espera",
				"type": "Wait",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"waitTimeInSeconds": 10
				}
			}
		],
		"folder": {
			"name": "adf_02_activities"
		},
		"annotations": [],
		"lastPublishTime": "2022-03-23T12:43:34Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}